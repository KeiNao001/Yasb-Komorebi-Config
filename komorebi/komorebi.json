{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.36/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/.config/komorebi/applications.json",
  "window_hiding_behaviour": "Cloak",
  "cross_monitor_move_behaviour": "Insert",
  "default_workspace_padding": 0,
  "default_container_padding": -1,
  "border": true,
  "border_width": 5,
  "border_offset": -1,
  "theme": {
    "palette": "Base16",
    "name": "Ashes",
    "unfocused_border": "Base01", 
    "stack_border": "Base0B",
    "stackbar_background": "Base00",
    "stackbar_focused_text": "Base0B",
    "stackbar_unfocused_text": "Base05"
  },
  "stackbar": {
    "height": 20,
    "mode": "OnStack",
    "label": "Process",
    "tabs": {
      "width": 150,
      "font_size": 18,
      "background": "#3C4841",
      "focused_text": "#A7C080",
      "font_family": "Inter"
    }
  },
  "animation": {
    "enabled": true,
    "duration": 100,
    "style": "EaseInCubic",
    "fps": 60
  },
  "mouse_follows_focus": false,
  "resize_delta": 100,
  "transparency": false,
  "transparency_alpha": 225,
  "transparency_ignore_rules": [
    {
      "kind": "Title",
      "id": "YouTube",
      "matching_strategy": "Contains"
    }
  ],
  "bar_configurations": [
    "$Env:KOMOREBI_CONFIG_HOME/komorebi.bar.json"
